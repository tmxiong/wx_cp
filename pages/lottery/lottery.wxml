<import src="../template/template.wxml" />

<view>
  <scroll-view>
    <view  wx:for="{{list}}" wx:for-item="item" wx:key="{{index}}">
      <navigator url="{{'/pages/lottery-list/lottery-list?type=' + item.code}}" hover-class="navigator-hover">
        <template is="lottery-list-gpc" data="{{...item}}" />
      </navigator>
    </view>
  </scroll-view>
</view>


